public class Main {
    public static void main(String[] args) throws Exception {
        System.out.println("hello, singly linked list");
		SinglyLinkedList list = new SinglyLinkedList();
		list.Append(17);
		list.Append(9);
		list.Append(5);
		list.Append(7);
		list.Append(10);
		
		list.InsertionSort(false);
		list.BubbleSort(true);
    }
}
